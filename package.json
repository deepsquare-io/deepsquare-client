{
  "name": "@deepsquare/deepsquare-client",
  "version": "0.15.2",
  "description": "Back-end package interacting with the DeepSquare Grid",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,sass,json,json5,md,mdx,yml,yaml}\"",
    "graphql:gen": "graphql-codegen --config codegen.yml",
    "build": "bun build.ts",
    "test:watch": "vitest",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "gen:eth": "scripts/generate-eth.sh"
  },
  "files": [
    "/dist",
    "/grpc"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./grpc/node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    },
    "./grpc/browser": {
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.11",
    "@protobuf-ts/grpc-transport": "^2.9.1",
    "@protobuf-ts/grpcweb-transport": "^2.9.1",
    "@protobuf-ts/runtime": "^2.9.1",
    "@protobuf-ts/runtime-rpc": "^2.9.1",
    "abitype": "^0.10.2",
    "async-lock": "^1.4.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "queueable": "^5.3.2",
    "viem": "^1.19.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/async-lock": "^1.4.2",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "bun-plugin-dts": "^0.2.1",
    "bun-types": "^1.0.13",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "graphql-tag": "^2.12.6",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
