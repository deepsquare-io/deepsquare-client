{
  "name": "@deepsquare/deepsquare-client",
  "version": "0.12.1",
  "description": "Back-end package interacting with the DeepSquare Grid",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,sass,json,json5,md,mdx,yml,yaml}\"",
    "graphql:gen": "graphql-codegen --config codegen.yml",
    "build": "rm -Rf .parcel-cache && parcel build",
    "test:watch": "vitest",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "gen:eth": "scripts/generate-eth.sh"
  },
  "source": "src/index.ts",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.mjs"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.16",
    "@protobuf-ts/grpc-transport": "^2.9.0",
    "@protobuf-ts/runtime": "^2.9.0",
    "@protobuf-ts/runtime-rpc": "^2.9.0",
    "async-lock": "^1.4.0",
    "graphql": "^15.8.0",
    "graphql-request": "^5.2.0",
    "viem": "^1.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/client-preset": "^1.3.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/core": "^2.9.2",
    "@parcel/packager-ts": "^2.9.2",
    "@parcel/transformer-typescript-types": "^2.9.2",
    "@types/async-lock": "^1.4.0",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "esbuild": "^0.18.6",
    "eslint": "^8.43.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql-tag": "^2.12.6",
    "parcel": "^2.9.2",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
